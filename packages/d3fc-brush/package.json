{
  "name": "@d3fc/d3fc-brush",
  "version": "2.0.67",
  "description": "Adapts the D3 brush, making it easier to create data-driven brushed charts.",
  "license": "MIT",
  "keywords": [
    "d3",
    "d3fc",
    "brush"
  ],
  "homepage": "https://github.com/d3fc/d3fc",
  "main": "build/d3fc-brush.js",
  "module": "index",
  "repository": {
